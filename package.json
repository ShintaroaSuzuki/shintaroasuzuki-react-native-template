{
    "name": "shintaroasuzuki-react-native-template",
    "version": "1.0.0",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "prepare:dev": "run-s submodule:update api:build mock:build",
        "dev:start": "cross-env API_URL=/api run-s prepare:dev start",
        "dev:ios": "cross-env API_URL=/api run-s prepare:dev ios",
        "dev:android": "cross-env API_URL=/api run-s prepare:dev android",
        "api:build": "run-s api:clean openapi2aspida",
        "api:clean": "rimraf src/apis/bin",
        "mock:build": "run-s openapiYml2json",
        "start": "expo start --dev-client",
        "android": "expo start --android --dev-client",
        "ios": "expo start --ios --dev-client",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "openapi2aspida": "openapi2aspida -c=aspida.config.cjs",
        "openapiYml2json": "sh src/apis/mocks/json/openapiYml2json.sh",
        "submodule:update": "git submodule update --remote",
        "prepare": "husky install",
        "lint-staged": "lint-staged --config .lintstagedrc.js"
    },
    "dependencies": {
        "@aspida/axios": "^1.12.0",
        "@aspida/react-query": "^1.12.0",
        "axios": "^1.4.0",
        "expo": "~48.0.18",
        "expo-status-bar": "~1.4.4",
        "react": "18.2.0",
        "react-native": "0.71.8",
        "react-native-mmkv": "^2.10.0",
        "react-query": "^3.39.3",
        "recoil": "^0.7.7"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@types/jest": "^29.5.2",
        "@types/react": "~18.0.14",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "axios-mock-adapter": "^1.21.5",
        "babel-plugin-module-resolver": "^5.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.44.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-native": "^4.0.0",
        "husky": "^8.0.3",
        "hygen": "^6.2.11",
        "jest": "^29.5.0",
        "lint-staged": "^13.2.3",
        "npm-run-all": "^4.1.5",
        "openapi2aspida": "^0.21.0",
        "prettier": "^2.8.8",
        "prh": "^5.4.4",
        "rimraf": "^5.0.1",
        "swagger-cli": "^4.0.4",
        "ts-jest": "^29.1.1",
        "typescript": "^4.9.4"
    },
    "private": true
}
